{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "cleanUrls": true,
  "trailingSlash": true,
  "rewrites": [{"source": "/api/(.*)", "destination": "/api/$1"}],
  "env": {
    "NEXT_PUBLIC_TEMPLATE_ID": "@next_public_template_id",
    "NEXT_PUBLIC_API_URL": "@next_public_api_url"
  }
}